
CREATE TABLE IF NOT EXISTS TaxRates (
    Id INTEGER PRIMARY KEY,
    StartTime TEXT NOT NULL,
    EndTime TEXT NOT NULL,
    Amount INTEGER NOT NULL
);

CREATE TABLE IF NOT EXISTS Holidays (
    Id INTEGER PRIMARY KEY,
    Date TEXT NOT NULL
);

